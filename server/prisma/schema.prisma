generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
}

model News {
  id Int      @id @default(autoincrement())
  title      String
  description String
  date       String
  author     String
}

model User {
  id Int      @id @default(autoincrement())
  name      String
  email String
  password       String
}